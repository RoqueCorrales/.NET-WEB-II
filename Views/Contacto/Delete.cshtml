@model Proyecto_Web_ll.Models.Contacto

@{
    ViewData["Title"] = "Delete";
}
@if(Session.isSingin){
<div class="container-fluid panel panel-default">

<h3>Are you sure you want to delete this contact?</h3>
<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Name:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt>
           LastName:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Apellido_Uno)
        </dd>
        <dt>
           Phone:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Telefono)
        </dd>
        <dt>
            Email:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Correo)
        </dd>
        <dt>
           Occupation:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Puesto)
        </dd>
        <dt>
           Client's Indentification:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Cliente.CedulaJuridica)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="ID" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
    <br>
</div>
</div>
}