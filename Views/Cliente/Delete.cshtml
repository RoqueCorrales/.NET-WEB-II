@model Proyecto_Web_ll.Models.Cliente

@{
    ViewData["Title"] = "Delete";
}

<div class="container-fluid panel panel-default">

<h3>Are you sure you want to delete this Client?</h3>
<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Name: 
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt>
          Identification:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CedulaJuridica)
        </dd>
        <dt>
            Web Page:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PaginaWeb)
        </dd>
        <dt>
            Address: 
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DireccionFisica)
        </dd>
        <dt>
           Phone:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.telefono)
        </dd>
        <dt>
            Sector:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Sector.Nombre)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="ID" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
    <br>
</div>
</div>