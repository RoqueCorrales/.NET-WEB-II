@model Proyecto_Web_ll.Models.Reunion

@{
    ViewData["Title"] = "Details";
}


<h2>Details Meeting</h2>



<div class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-6 col-md-6">
            <div class="well well-sm">
                <div class="row">
                    <div class="col-sm-6 col-md-4">
                        <img src="~/images/logo.png" alt="" class="img-rounded img-responsive" />
                    </div>
                    <div class="col-sm-6 col-md-8">
                        <h4>
                            Title:  @Html.DisplayFor(model => model.Titulo)</h4>
                       
                        <p>
                            <i class="glyphicon glyphicon-envelope"></i>Date:  @Html.DisplayFor(model => model.EnrollmentDate)
                            <br />
                            <i class="glyphicon glyphicon-globe"></i><a> Virtual? @Html.DisplayFor(model => model.Virtual)</a>
                            <br />
                            <i class="glyphicon glyphicon-gift"></i>Identification Client  @Html.DisplayFor(model => model.Cliente.CedulaJuridica)</p>
                            
                            <i class="glyphicon glyphicon-gift"></i>  User in charge:  @Html.DisplayFor(model => model.Usuario.Nombre) @Html.DisplayFor(model => model.Usuario.Apellido_Uno)</p>
                            <br>
                          <a asp-action="Edit" asp-route-id="@Model.ID">Edit</a> |
                           <a asp-action="Index">Back to List</a>
                    </div>

 
                </div>
            </div>
        </div>
    </div>



</div>
