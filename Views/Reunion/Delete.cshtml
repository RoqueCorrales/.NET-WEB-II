@model Proyecto_Web_ll.Models.Reunion

@{
    ViewData["Title"] = "Delete";
}

<div class="container-fluid panel panel-default">

<h3>Are you sure you want to delete this meeting?</h3>
<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
          Title:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Titulo)
        </dd>
        <dt>
           Date:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.EnrollmentDate)
        </dd>
        <dt>
            Virtual?
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Virtual)
        </dd>
        <dt>
            Client's Identification:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Cliente.CedulaJuridica)
        </dd>
        <dt>
           Password:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Usuario.Contrasenna)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="ID" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
    <br>
    </div>
</div>
